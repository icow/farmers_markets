      <th>Trees</th>
      <th>Wine</th>
      <th>Update time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @markets.each do |market| %>
      <tr>
        <td><%= market.FMID %></td>
        <td><%= market.market_name %></td>
        <td><%= market.website %></td>
        <td><%= market.street %></td>
        <td><%= market.city %></td>
        <td><%= market.county %></td>
        <td><%= market.state %></td>
        <td><%= market.zip %></td>
        <td><%= market.season1_date %></td>
        <td><%= market.season1_time %></td>
        <td><%= market.season2_date %></td>
        <td><%= market.season2_time %></td>
